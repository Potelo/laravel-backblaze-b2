{
    "name": "gliterd/laravel-backblaze-b2",
    "type": "library",
    "description": "Backblaze B2 Cloud Storage for Laravel 5",
    "keywords": ["flysystem", "filesystem", "api", "backblaze", "laravel", "storage", "disks"],
    "homepage": "https://github.com/mhetreramesh/flysystem-backblaze",
    "license": "MIT",
    "authors": [
        {
            "name": "Ramesh Mhetre",
            "email": "mhetreramesh@gmail.com",
            "homepage": "https://about.me/rameshmhetre",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
          "type": "git",
          "url": "https://github.com/andrewalkermo/flysystem-backblaze.git"
        }
    ],
    "require": {
        "php": "^7.1.8 || ^8.0",
        "mhetreramesh/flysystem-backblaze": "dev-php-8",
        "illuminate/support": "^5.5 || ^6.0 || ^7.0 || ^8.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0||~6.0||~7.0||~8.0||~9.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Gliterd\\BackblazeB2\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gliterd\\BackblazeB2\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "format": "phpcbf --standard=psr2 src/"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "\\Gliterd\\BackblazeB2\\BackblazeB2ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
